package test_0801;

public class For {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		//2) for
		// (int)(Math.random()*100) 을 이용해서 더하기 문제를 5번 제출하세요
		
		int a,b = 0;
		
		for (int i = 0; i<100 ; i++) {
			int x = (int)Math.random()*100;
			int y = (int)Math.random()*100;
			
			System.out.println(x+"+" + y + "=");
		}
		 
		
	}

}
