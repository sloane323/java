package aug.Test;

public class Student {
	
	String name;
	int number;
	int score;
	
	Student() {
		this.name = "홍길동";
		this.number= 0;
		this.score = 0;
	}
	
	Student (String name, int number)
	{
		this.name=name;
		this.number=number;
		this.score =0;
	
	}
	
	void setScore(int score) {
		this.score = score;
	}
	
	int getScore() {
		return this.score;
	}
	
}
